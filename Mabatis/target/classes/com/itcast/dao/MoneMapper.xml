<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itcast.dao.MoneMapper">

    <select id="getUser" resultType="mone">
        select * from mone;
    </select>

    <insert id="moneySave"  parameterType="mone">
        INSERT INTO mone VALUE(#{id},#{username},#{money});
    </insert>
</mapper>
